  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch test function",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${fileDirname}",
      "args": ["-test.run", "TestCreateTodoMutation"]
    },
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceRoot}",
      "program": "${fileDirname}",

      "env": {},
      "args": [
        "--queries",
        "internal/integration/**/*.graphql",
        "--schema",
        "internal/integration/graph/schema.graphqls",
        "--destination",
        "./internal/integration/client.go",
        "--packageName",
        "integration"
      ]
    }
  ]
}
